<Window x:Class="_1_Projekt_Textbox_Massagebox_Ausgabe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1_Projekt_Textbox_Massagebox_Ausgabe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtInputOutput" HorizontalAlignment="Center" Margin="0,81,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="274" Height="117" TextChanged="txtAusgabe_TextChanged"/>
        <Button x:Name="KlickHere" Content="Klick Here" Margin="289,242,275,101" RenderTransformOrigin="0.5,0.5" Click="KlickHere_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.334"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
</Window>
